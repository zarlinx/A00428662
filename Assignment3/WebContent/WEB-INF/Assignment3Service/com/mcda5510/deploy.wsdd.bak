<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from Assignment3Service WSDL service -->

  <service name="Assignment3" provider="java:RPC" style="document" use="encoded">
      <parameter name="wsdlTargetNamespace" value="http://mcda5510.com"/>
      <parameter name="wsdlServiceElement" value="Assignment3Service"/>
      <parameter name="wsdlServicePort" value="Assignment3"/>
      <parameter name="className" value="com.mcda5510.Assignment3SoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Assignment3"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <parameter name="allowedMethods" value="*"/>

  </service>
</deployment>
